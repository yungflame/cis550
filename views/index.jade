head
  meta(charset='UTF-8')
  title CIS 550 Group 33 Project App
  script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.3.3/angular.min.js')
  script(src='//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.3/angular-route.min.js')
  script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js')
  link(rel='stylesheet', href='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css')
  script(src='//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js')
  script(src='script.js')
body
  div
    div
      nav.navbar.navbar-inverse(role='navigation', style='padding-left:100px;')
        ul.nav.navbar-nav
          li.active
            a(href='/')
              | Home
              span.sr-only (current)
          li
            a(href='/about') About Us
          li
            a(href='/contact') Contact Us
    .page-header.text-center
      h1 #{title}
    // page-header
    .container(style='max-width:800px')
      .row.text-center
        .col-sm-12
          img.img-responsive(src='http://www.quickanddirtytips.com/sites/default/files/images/5360/line_graph.png')
          h3 Sample Graph 1
          p Brief caption
        // col-sm-12
      // row
      br
      br
      .row.text-center
        .col-sm-12
          img.img-responsive(src='http://www.glowscript.org/docs/GlowScriptDocs/images/graph.jpg')
          h3 Sample Graph 2
          p Brief caption
        // col-sm-12
      // row
      .row
        .col-sm-12
          table
            - for (var inx = 0; inx < results.length; inx++)
              tr
                td=results[inx][NAME]+" "
                td &nbsp;
        // col-sm-12
      // row
      div(style='height:100px')
    // container
